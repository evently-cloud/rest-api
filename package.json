{
  "name": "@evently/rest-api",
  "version": "0.0.1",
  "description": "Evently REST API",
  "main": "dist/index.js",
  "author": "Matt Bishop",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist yarn-error.log",
    "generate-types": "json2ts --cwd schema -i schema -o src/api/type --no-style.semi",
    "launch": "tsx --env-file ./env/evently.env src/index.ts",
    "debug": "tsx --env-file ./env/evently.env src/index.ts",
    "start": "node --trace-warnings --env-file ./env/evently.env dist/src/index.js",
    "profile": "node --env-file ./env/evently.env --trace_gc dist/index.js",
    "trace-events": "tsx --env-file ./env/evently.env --trace-event-categories v8,node,node.async_hooks dist/index.js"
  },
  "devDependencies": {
    "@types/http-errors": "2.0.5",
    "@types/lodash-es": "4.17.12",
    "@types/node": "24.8.1",
    "json-schema-to-typescript": "15.0.4",
    "pino-pretty": "13.1.2",
    "tsx": "4.20.6",
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@fastify/auth": "5.0.3",
    "@fastify/compress": "8.1.0",
    "@fastify/cors": "11.1.0",
    "@fastify/request-context": "6.2.1",
    "@js-joda/core": "5.6.5",
    "@paralleldrive/cuid2": "3.1.0",
    "close-with-grace": "2.3.0",
    "fast-json-stringify": "6.1.1",
    "fast-rbac": "2.0.1",
    "fastify": "5.6.1",
    "fastify-allow": "2.1.1",
    "fastify-https-always": "2.0.0",
    "fastify-plugin": "5.1.0",
    "fastify-sse-v2": "4.2.1",
    "http-errors": "2.0.0",
    "iter-ops": "3.5.0",
    "lodash-es": "4.17.21",
    "long": "5.3.2",
    "msgpackr": "1.11.5",
    "pino": "10.1.0",
    "postgres": "3.4.7",
    "specified": "1.0.2",
    "sql-jsonpath-js": "1.0.1",
    "toad-cache": "3.7.0",
    "weakref": "0.2.1"
  }
}
