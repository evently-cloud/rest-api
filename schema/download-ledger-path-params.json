{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "title": "Download Ledger Path Parameters",
  "properties": {
    "id": {
      "type": "string",
      "pattern": "^[0-9a-fA-F]{8}$"
    },
    "select": {
      "type": "string",
      "pattern": "^[0-9A-Za-z_-]{2,}\\.ndjson$"
    }
  },
  "additionalProperties": false,
  "required": ["id", "select"]
}
